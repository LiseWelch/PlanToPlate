@model RecipeView

<div class="CustomBar col-sm-8 offset-sm-2" id="ItemList" style="max-height: 50vh">
        <ul>
            @{
                foreach (Ingriedient i in @Model.items)
                    {
                        <li>@i.Name @i.AmountNum @i.AmountString</li>
                    }
            }
        </ul>
</div>